{
  "name": "acme",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0 <25",
    "pnpm": ">=9.15.9"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc",
  "scripts": {
    "build": "turbo run build",
    "check": "turbo run check --continue -- --write --unsafe",
    "check:dependencies": "pnpm dlx sherif@latest",
    "check:dependencies:fix": "pnpm dlx sherif@latest --fix",
    "clean": "turbo run clean && git clean -xdf .turbo node_modules",
    "db:push": "turbo -F @acme/db push",
    "db:studio": "turbo -F @acme/db studio",
    "dev": "turbo watch dev --continue",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@biomejs/biome": "~2.4.4",
    "@turbo/gen": "~2.8.10",
    "turbo": "~2.8.10"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "zod": "^3.23.8 || ^4.0.0"
      },
      "ignoreMissing": [
        "@better-auth/passkey",
        "@daveyplate/better-auth-tanstack",
        "@instantdb/react",
        "@marsidev/react-turnstile",
        "@triplit/client",
        "@triplit/react"
      ]
    }
  }
}
