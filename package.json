{
  "name": "acme",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0 <25",
    "pnpm": ">=9.15.9"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc",
  "scripts": {
    "build": "turbo run build",
    "check": "turbo run check --continue -- --write --unsafe",
    "check:dependencies": "pnpm dlx sherif@latest -i ai@~4.3.19 -i @ai-sdk/openai@~1.3.22",
    "check:dependencies:fix": "pnpm dlx sherif@latest --fix -i ai@~4.3.19 -i @ai-sdk/openai@~1.3.22",
    "clean": "turbo run clean && git clean -xdf .turbo node_modules",
    "db:push": "turbo -F @acme/db push",
    "db:studio": "turbo -F @acme/db studio",
    "dev:next": "turbo watch dev -F @acme/nextjs...",
    "dev": "turbo watch dev --continue",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@biomejs/biome": "~2.2.4",
    "@turbo/gen": "~2.5.8",
    "turbo": "~2.5.8"
  },
  "pnpm": {
    "overrides": {
      "@better-fetch/fetch": "1.1.18"
    }
  }
}
