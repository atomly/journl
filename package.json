{
  "name": "journl",
  "private": true,
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=9.6.0"
  },
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "auth:generate": "pnpm -F @acme/auth generate",
    "build": "turbo run build",
    "check:fix": "turbo run check --continue -- --write",
    "check": "turbo run check --continue",
    "clean:workspaces": "turbo run clean",
    "clean": "git clean -xdf node_modules",
    "db:push": "turbo -F @acme/db push",
    "db:studio": "turbo -F @acme/db studio",
    "dependencies:fix": "pnpm dlx sherif@latest --fix",
    "dependencies": "pnpm dlx sherif@latest",
    "dev:next": "turbo watch dev -F @acme/nextjs...",
    "dev": "turbo watch dev --continue",
    "postinstall": "pnpm dependencies",
    "typecheck": "turbo run typecheck",
    "ui-add": "turbo run ui-add"
  },
  "devDependencies": {
    "@turbo/gen": "^2.5.4",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  }
}
