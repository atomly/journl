{
  "name": "acme",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=22.20.0",
    "pnpm": ">=9.6.0"
  },
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "auth:generate": "pnpm -F @acme/auth generate",
    "build": "turbo run build",
    "check": "turbo run check --continue -- --write --unsafe",
    "clean": "turbo run clean && git clean -xdf .turbo node_modules",
    "db:push": "turbo -F @acme/db push",
    "db:studio": "turbo -F @acme/db studio",
    "dependencies": "pnpm dlx sherif@latest --fix -i ai@~4.3.19 -i @ai-sdk/openai@~1.3.22",
    "dependencies:check": "pnpm dlx sherif@latest -i ai@~4.3.19 -i @ai-sdk/openai@~1.3.22",
    "dev:next": "turbo watch dev -F @acme/nextjs...",
    "dev": "turbo watch dev --continue",
    "postinstall": "pnpm dependencies:check",
    "shadcn": "turbo run shadcn",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@biomejs/biome": "~2.2.4",
    "@turbo/gen": "~2.5.8",
    "turbo": "~2.5.8"
  }
}
